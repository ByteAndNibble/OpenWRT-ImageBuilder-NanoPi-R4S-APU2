/dts-v1/;

/ {
	description = "MIPS OpenWrt FIT (Flattened Image Tree)";
	#address-cells = <1>;

	images {
		kernel-1 {
			description = "MIPS OpenWrt Linux-5.15.137";
			data = /incbin/("/builder/shared-workdir/build/build_dir/target-mipsel_24kc_musl/linux-ramips_mt7621/h3c_tx1801-plus-kernel.bin");
			type = "kernel";
			arch = "mips";
			os = "linux";
			compression = "lzma";
			load = <0x82000000>;
			entry = <0x82000000>;
			hash-1 {
				algo = "crc32";
			};
			hash-2 {
				algo = "sha1";
			};
		};


		fdt-1 {
			description = "MIPS OpenWrt h3c_tx1801-plus device tree blob";
			
			data = /incbin/("/builder/shared-workdir/build/build_dir/target-mipsel_24kc_musl/linux-ramips_mt7621/image-mt7621_h3c_tx1801-plus.dtb");
			type = "flat_dt";
			
			arch = "mips";
			compression = "none";
			hash-1 {
				algo = "crc32";
			};
			hash-2 {
				algo = "sha1";
			};
		};



	};

	configurations {
		default = "config-1";
		config-1 {
			description = "OpenWrt h3c_tx1801-plus";
			kernel = "kernel-1";
			fdt = "fdt-1";
			
			
			
		};
		
	};
};
